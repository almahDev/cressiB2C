    "title": "Imagens - selos de segurança",
    "props": {
      "itemsPerPage": {
        "desktop": 2,
        "tablet": 2,
        "phone": 2
      },
      "infinite": false,
      "label": "highlight-badges-slider",
      "fullWidth": false,
      "centerMode": "to-the-left",
      "showPaginationDots": "never",
      "showNavigationArrows": "never",
      "blockClass": "security-slider"
    }
