  "slider-layout#payment-methods-images-top": {
    "title": "Imagens - Bandeiras superiores",
    "props": {
      "itemsPerPage": {
        "desktop": 6,
        "tablet": 6,
        "phone": 6
      },
      "blockClass": ["payment-methods-images"],
      "infinite": false,
      "label": "payment-flags-slider",
      "fullWidth": false,
      "centerMode": "to-the-left",
      "centerModeSlidesGap": 6
    }
  },
  "slider-layout#payment-methods-images-bottom": {
