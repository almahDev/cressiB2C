    "children": ["slider-layout#highlight-images"],
    "props": {
      "preload": false,
      "height": 56,
      "mediaList": [
        {
          "image": "assets/img/seguranca/pci.png",
          "mobileImage": "assets/img/seguranca/pci.png",
          "description": "brasão",
          "width": "65px"
        },
        {
          "image": "assets/img/seguranca/lets-encrypt.png",
          "mobileImage": "assets/img/seguranca/lets-encrypt.png",
          "description": "brasão",
          "width": "61px"
        }
      ]
