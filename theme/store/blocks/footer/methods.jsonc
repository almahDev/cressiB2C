{
  "flex-layout.row#footer-methods": {
    "title": "Footer  Selos",
    "children": [
      "flex-layout.col#payment-flags",
      "flex-layout.col#security-badges",
      "flex-layout.col#social-networks-badges"
    ],
    "props": {
      "blockClass": ["footer_methods-", "footer_methods"]
    }
  },

  // payment methods
  "flex-layout.col#payment-flags": {
    "title": "Footer  Selos - Pagamento",
    "children": [
      "rich-text#footer-payment-title",
      "flex-layout.row#payment-methods-top",
      "flex-layout.row#payment-methods-bottom"
    ],
    "props": {
      "blockClass": ["payment-flags-wrapper", "home__footer--high"],
      "horizontalAlign": "left",
      "marginLeft": 0,
      "marginRight": 0,
      "paddingLeft": 0,
      "paddingRight": 2,
      "preventVerticalStretch": true,
      "rowGap": 0,
      "verticalAlign": "top",
      "width": "37%"
    }
  },
  "rich-text#footer-payment-title": {
    "title": "Footer  Selos - Pagamento - Título",
    "props": {
      "text": "### Formas de pagamento",
      "blockClass": "footer_methods--title"
    }
  },
  "flex-layout.row#payment-methods-top": {
    "children": ["list-context.media-list#payment-methods-top"],
    "props": {
      "blockClass": ["payment-methods-flags"],
      "colGap": 0,
      "colSizing": "equal",
      "fullWidth": true,
      "horizontalAlign": "left",
      "marginBottom": 1,
      "marginTop": 0,
      "paddingBottom": 1,
      "paddingTop": 0,
      "preserveLayoutOnMobile": true,
      "preventHorizontalStretch": false,
      "preventVerticalStretch": true,
      "rowGap": 0
    }
  },
  "flex-layout.row#payment-methods-bottom": {
    "children": ["list-context.media-list#payment-methods-bottom"],
    "props": {
      "blockClass": "payment-methods-flags",
      "colGap": 0,
      "colSizing": "equal",
      "fullWidth": true,
      "horizontalAlign": "left",
      "marginBottom": 1,
      "marginTop": 0,
      "paddingBottom": 1,
      "paddingTop": 1,
      "preserveLayoutOnMobile": true,
      "preventHorizontalStretch": false,
      "preventVerticalStretch": true,
      "rowGap": 0
    }
  },
